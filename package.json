{
  "name": "cycle-firebase-driver",
  "version": "0.0.0",
  "description": "A Cycle.js driver for Firebase.",
  "author": "",
  "main": "lib/index.js",
  "scripts": {
    "clean": "rm -rf ./build/ && mkdir ./build/",
    "build": "npm run clean && browserify ./test/index.js --debug -t babelify --outfile ./build/app.js",
    "precompile-lib": "rm -rf ./lib/ && mkdir -p ./lib/",
    "compile-lib": "babel -d ./lib/ ./src/",
    "prepublish": "npm run compile-lib"
  },
  "browserify-shim": {
    "rx": "global:Rx"
  },
  "devDependencies": {
    "@cycle/core": "6.0.2",
    "@cycle/dom": "9.1.0",
    "@cycle/http": "8.2.0",
    "@cycle/isolate": "1.2.0",
    "babel": "6.5.2",
    "babel-cli": "6.5.1",
    "babel-preset-es2015": "6.5.0",
    "babel-preset-stage-0": "6.5.0",
    "babelify": "7.2.0",
    "browserify": "13.0.0",
    "browserify-shim": "3.8.12",
    "csjs": "1.0.0-beta.4",
    "csjs-inject": "1.0.0-beta.0",
    "firebase": "2.4.1",
    "rx": "4.0.8",
    "uglify-js": "2.6.2"
  },
  "license": "MIT"
}
