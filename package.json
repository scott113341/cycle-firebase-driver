    "babel-preset-es2015": "6.6.0",
    "babel-preset-stage-0": "6.5.0",
    "babelify": "7.2.0",
    "browser-run": "3.1.0",
    "browserify": "13.0.0",
    "browserify-shim": "3.8.12",
    "csjs": "1.0.1",
    "csjs-inject": "1.0.0",
    "lodash.isstring": "4.0.1",
